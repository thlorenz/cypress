#!/usr/bin/env node
// @ts-check

const config = require('../snapconfig')('prod')

if (process.env.DEBUG == null) {
  process.env.DEBUG = config.LOG_DEBUG
}

const setup = require('../lib/setup')

;(async () => {
  try {
    await setup(config)
  } catch (err) {
    process.exit(err.code ?? 1)
  }
})()
